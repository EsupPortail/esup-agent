<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd">
	<bean id="configAgent" class="org.esupportail.esupAgent.domain.beans.config.ConfigAgent">
		<description>Contient les éléments de configuration du canal
			Agent</description>
		<property name="domainService" ref="domainService"/>
		
		<property name="admins_string" 
			value="${agent.admin}">
		</property>
		<property name="wsdl_url_referentiel_geographique"
			value="${wsdl.url_referentiel_geographique}">
		</property>
		<property name="wsdl_url_referentiel_rh"
			value="${wsdl.url_referentiel_rh}">
		</property>
		<property name="wsdl_url_dossier_rh_personnel"
			value="${wsdl.url_dossier_rh_personnel}">
		</property>
		<property name="wsdl_url_dossier_rh_administratif"
			value="${wsdl.url_dossier_rh_administratif}">
		</property>


	</bean>

</beans>